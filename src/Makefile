# all objects
OBJS = tester.o main.o

# flags
CFLAGS =-g -03 # -g for debug, -03 for optimization


# first production
all: tokenizer

# tokenizer production
tokenizer: $(OBJS)
	cc -o tokenizer $(CFLAGS) $(OBJS)

# all object files are dependent on tokenizer.h, history.h
$(OBJS): tokenizer.h history.h

# delete files generated by compilation
clean:
	rm -f *.o tokenizer

# Running Program
run: tokenizer
